issn = {16993055},
keywords = {Chemotherapy,Glioblastoma,Prognosis,Radiotherapy,Survival,Tumor marker},
month = {nov},
number = {11},
pages = {1062--1071},
pmid = {26960561},
publisher = {Springer-Verlag Italia s.r.l.},
title = {{Survival in glioblastoma: a review on the impact of treatment modalities}},
