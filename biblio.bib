doi = {10.1007/s13277-016-5331-4},
issn = {14230380},
journal = {Tumor Biology},
number = {11},
title = {{Transcription factor HIF1A: downstream targets, associated pathways, polymorphic hypoxia response element (HRE) sites, and initiative for standardization of reporting in scientific literature}},
