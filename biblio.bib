journal = {American Journal of Physiology - Cell Physiology},
keywords = {Cell cycle,Chaperonemediated autophagy,Cyclin-dependent kinases,Hematopoietic stem cells,Hypoxia-inducible factor-1,Minichromosome maintenance helicase,Neural stem cells},
