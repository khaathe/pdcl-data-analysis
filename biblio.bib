issn = {1933-0693},
journal = {Journal of Neurosurgery},
keywords = {Biomarker,Cancer stem-like cells,Collagen,Glioblastoma,Oncology,Second-harmonic generation},
mendeley-groups = {Biblio/ECM},
month = {sep},
number = {6},
pages = {1812--1821},
pmid = {27588592},
publisher = {American Association of Neurological Surgeons},
title = {{Association of collagen architecture with glioblastoma patient survival}},
url = {https://thejns.org/view/journals/j-neurosurg/126/6/article-p1812.xml},
volume = {126},
year = {2016}
}
@article{Kaufman2005,
abstract = {We study the growth and invasion of glioblastoma multiforme (GBM) in three-dimensional collagen I matrices of varying collagen concentration. Phase-contrast microscopy studies of the entire GBM system show that invasiveness at early times is limited by available collagen fibers. At early times, high collagen concentration correlates with more effective invasion. Conversely, high collagen concentration correlates with inhibition in the growth of the central portion of GBM, the multicellular tumor spheroid. Analysis of confocal reflectance images of the collagen matrices quantifies how the collagen matrices differ as a function of concentration. Studying invasion on the length scale of individual invading cells with a combination of confocal and coherent anti-Stokes Raman scattering microscopy reveals that the invasive GBM cells rely heavily on cell-matrix interactions during invasion and remodeling. {\textcopyright} 2005 by the Biophysical Society.},
author = {Kaufman, Laura J. and Brangwynne, C. P. and Kasza, K. E. and Filippidi, E. and Gordon, V. D. and Deisboeck, T. S. and Weitz, D. A.},
