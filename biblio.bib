title = {{Kinetic analysis and design of experiments to identify the catalytic mechanism of the monocarboxylate transporter isoforms 4 and 1}},
url = {http://www.cell.com/article/S0006349510051854/fulltext http://www.cell.com/article/S0006349510051854/abstract https://www.cell.com/biophysj/abstract/S0006-3495(10)05185-4},
volume = {100},
