pages = {13164--13174},
pmid = {23679052},
publisher = {American Chemical Society},
title = {{Tristability in cancer-associated microRNA-TF chimera toggle switch}},
url = {https://pubs.acs.org/doi/abs/10.1021/jp403156m},
volume = {117},
year = {2013}
}
