doi = {10.3390/metabo8010004},
file = {:home/spinicck/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Cuperlovic-Culf - 2018 - Machine learning methods for analysis of metabolic data and metabolic pathway modeling.pdf:pdf},
